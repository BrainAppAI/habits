{
    "manifest_version": 3,
    "name": "Habits by Brain",
    "version": "1.1.0",
    "description": "Track your habits in every new tab",
    "permissions": ["storage"],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    }
}
